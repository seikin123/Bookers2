<h1>User info#index</h1>
<%= render 'users/profile', user: @user %>
<h3>New book</h3>
<%= render 'books/form', {book: @booknew, btn: 'Create Book'} %>

<table>
	<h2>Users</h2>
    <thead>
      <th>image</th>
      <th>name</th>
      <th></th>
</thead>
<tbody>
<% @users.each do |user| %>
<tr>
<td><%= attachment_image_tag user, :profile_image, :fill, 50, 50,fallback: "no_image.jpg" %></td>
<td><%= user.name %></td>
<td><%= link_to "Show", "/users/#{user.id}" %></td>
</tr>
<% end %>
</tbody>
</table>








